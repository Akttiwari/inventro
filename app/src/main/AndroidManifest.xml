<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="example.akshat.com.inventro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="inventro"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="example.akshat.com.inventro.MainActivity"
            android:label="inventro"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="example.akshat.com.inventro.AddMartialArtActivity"/>
        <activity android:name="example.akshat.com.inventro.UpdateMartialArtActivity" />
        <activity android:name="example.akshat.com.inventro.DeleteMartialArt"></activity>
    </application>

</manifest>